		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-hero-icons": "^4.1.1",
		"svelte-preprocess": "^4.10.5",
		"tailwindcss": "^3.0.23",
		"tslib": "^2.3.1",
		"typescript": "~4.6.2"
